            "libboost-serialization1.71.0 | libboost-all-dev",
            "librocksdb-dev | librocksdb5.17",
            "libtbb2 | libtbb-dev",
            "fuse3"
        ],
        "el": []
    },
    "releases": [
        {
            "image": "autotier-ubuntu-focal-builder",
            "codeName": "focal",
            "type": "deb"
        },
        {
            "image": "autotier-debian-bullseye-builder",
            "codeName": "bullseye",
            "type": "deb"
        },
        {
            "image": "autotier-rocky-el8-builder",
            "codeName": "el8",
            "type": "el"
